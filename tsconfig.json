{
    "compilerOptions": {
        "moduleResolution": "nodenext",
        "removeComments": true,
        "target": "es2022",

        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
        "useUnknownInCatchVariables": true
    }
}
